    if ((buildings.Fishermans = 0) and (buildings.Woodcutters >= 5) and (buildings.Metallurgists > 0) and (buildings.Inn > 0) and (buildings.Farm > 2)) then
    begin
      result := BH_AddBuildingInfo(result, htFishermans);
    end;
  end;

